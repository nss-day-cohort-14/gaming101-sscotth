doctype
html
  head
    title Tickie Tackie Toe
    link(rel='stylesheet', href='/main.css')
  body
    h1 Tickie Tackie Toe
    .create
      a(href='/game/create') Start new game
    if games.length
      .join
        h2 Join a game
        for game in games
          li: a(href=`/game/${game._id}`) Game: #{game._id}
